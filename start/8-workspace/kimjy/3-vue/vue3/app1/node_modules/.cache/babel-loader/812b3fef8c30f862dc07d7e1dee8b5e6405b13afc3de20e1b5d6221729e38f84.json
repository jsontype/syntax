{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, reactive, onMounted } from \"vue\";\nconst todos = reactive([]);\n// setTodos([...todos, newTodo]) // 리액트 setState\n// todos.push(newTodo) // 뷰 ref\n\nconst newTodo = ref(\"\");\n// const [newTodo] = useState(\"\") // 리액트 state\n// newTodo.current = \"바꿀 값\" // 리액트 ref\n// newTodo.value = \"바꿀 값\" // 뷰 ref\n\n// * TODO (예시): 아래의 초기값을, \"로그인했을 때의 회원아이디\"로 변경할 것\nconst loginUserId = ref(1);\nfunction getTodos() {\n  fetch(\"https://jsonplaceholder.typicode.com/todos\").then(res => res.json()).then(json => {\n    console.log(json);\n    for (let i = 0; i < json.length; i++) {\n      if (json[i].userId === loginUserId.value) {\n        todos.push(json[i]); // reactive로 선언을 할 때는 value를 사용하지 않는다.\n      }\n    }\n    console.log(todos[0]);\n    console.log(todos[1]);\n    console.log(todos[2]);\n  });\n}\nfunction toggleTodo(item) {\n  item.completed = !item.completed;\n}\nfunction addTodo() {\n  if (newTodo.value === \"\") {\n    alert(\"할 일을 입력해주세요.\");\n    return;\n  }\n  const todo = {\n    id: todos.length + 1,\n    title: newTodo.value,\n    completed: false,\n    userId: loginUserId.value\n  };\n  todos.push(todo);\n}\nfunction delTodo(id) {\n  // 해당 id를 가진 todo 찾기\n  const index = todos.findIndex(todo => todo.id === id);\n  // 찾은 todo가 있으면 삭제\n  todos.splice(index, 1);\n  console.log(todos);\n}\nonMounted(() => {\n  getTodos();\n});\n\n// onUnmounted(() => {\n//   console.log(\"TodoApp unmounted\")\n// })","map":{"version":3,"names":["ref","reactive","onMounted","todos","newTodo","loginUserId","getTodos","fetch","then","res","json","console","log","i","length","userId","value","push","toggleTodo","item","completed","addTodo","alert","todo","id","title","delTodo","index","findIndex","splice"],"sources":["/Users/dev/Desktop/workspace/syntax/start/8-workspace/kimjy/3-vue/vue3/app1/src/components/TodoApp.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>Todo App</h1>\n    <div id=\"addTodo\">\n      <input type=\"text\" v-model=\"newTodo\" />\n      <button @click=\"addTodo\">추가</button>\n    </div>\n    <div id=\"todos\">\n      <div v-for=\"todo in todos\" :key=\"todo.id\" class=\"todo\">\n        <span @click=\"toggleTodo(todo)\">\n          <span>#{{ todo.id }} / </span>\n          <span>{{ todo.title }} / </span>\n          <input type=\"checkbox\" :checked=\"todo.completed\" />\n        </span>\n        <span>\n          <button @click=\"delTodo(todo.id)\">삭제</button>\n        </span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, reactive, onMounted } from \"vue\"\n\nconst todos = reactive([])\n// setTodos([...todos, newTodo]) // 리액트 setState\n// todos.push(newTodo) // 뷰 ref\n\nconst newTodo = ref(\"\")\n// const [newTodo] = useState(\"\") // 리액트 state\n// newTodo.current = \"바꿀 값\" // 리액트 ref\n// newTodo.value = \"바꿀 값\" // 뷰 ref\n\n// * TODO (예시): 아래의 초기값을, \"로그인했을 때의 회원아이디\"로 변경할 것\nconst loginUserId = ref(1)\n\nfunction getTodos() {\n  fetch(\"https://jsonplaceholder.typicode.com/todos\")\n    .then((res) => res.json())\n    .then((json) => {\n      console.log(json)\n      for (let i = 0; i < json.length; i++) {\n        if (json[i].userId === loginUserId.value) {\n          todos.push(json[i]) // reactive로 선언을 할 때는 value를 사용하지 않는다.\n        }\n      }\n      console.log(todos[0])\n      console.log(todos[1])\n      console.log(todos[2])\n    })\n}\n\nfunction toggleTodo(item) {\n  item.completed = !item.completed\n}\n\nfunction addTodo() {\n  if (newTodo.value === \"\") {\n    alert(\"할 일을 입력해주세요.\")\n    return\n  }\n  const todo = {\n    id: todos.length + 1,\n    title: newTodo.value,\n    completed: false,\n    userId: loginUserId.value,\n  }\n  todos.push(todo)\n}\n\nfunction delTodo(id) {\n  // 해당 id를 가진 todo 찾기\n  const index = todos.findIndex((todo) => todo.id === id)\n  // 찾은 todo가 있으면 삭제\n  todos.splice(index, 1)\n  console.log(todos)\n}\n\nonMounted(() => {\n  getTodos()\n})\n\n// onUnmounted(() => {\n//   console.log(\"TodoApp unmounted\")\n// })\n</script>\n\n<style lang=\"css\" scoped>\n.movieTitle {\n  display: block;\n}\n</style>\n"],"mappings":";AAuBA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AAE7C,MAAMC,KAAI,GAAIF,QAAQ,CAAC,EAAE;AACzB;AACA;;AAEA,MAAMG,OAAM,GAAIJ,GAAG,CAAC,EAAE;AACtB;AACA;AACA;;AAEA;AACA,MAAMK,WAAU,GAAIL,GAAG,CAAC,CAAC;AAEzB,SAASM,QAAQA,CAAA,EAAG;EAClBC,KAAK,CAAC,4CAA4C,EAC/CC,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAAEE,IAAI,IAAK;IACdC,OAAO,CAACC,GAAG,CAACF,IAAI;IAChB,KAAK,IAAIG,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIH,IAAI,CAACI,MAAM,EAAED,CAAC,EAAE,EAAE;MACpC,IAAIH,IAAI,CAACG,CAAC,CAAC,CAACE,MAAK,KAAMV,WAAW,CAACW,KAAK,EAAE;QACxCb,KAAK,CAACc,IAAI,CAACP,IAAI,CAACG,CAAC,CAAC,GAAE;MACtB;IACF;IACAF,OAAO,CAACC,GAAG,CAACT,KAAK,CAAC,CAAC,CAAC;IACpBQ,OAAO,CAACC,GAAG,CAACT,KAAK,CAAC,CAAC,CAAC;IACpBQ,OAAO,CAACC,GAAG,CAACT,KAAK,CAAC,CAAC,CAAC;EACtB,CAAC;AACL;AAEA,SAASe,UAAUA,CAACC,IAAI,EAAE;EACxBA,IAAI,CAACC,SAAQ,GAAI,CAACD,IAAI,CAACC,SAAQ;AACjC;AAEA,SAASC,OAAOA,CAAA,EAAG;EACjB,IAAIjB,OAAO,CAACY,KAAI,KAAM,EAAE,EAAE;IACxBM,KAAK,CAAC,cAAc;IACpB;EACF;EACA,MAAMC,IAAG,GAAI;IACXC,EAAE,EAAErB,KAAK,CAACW,MAAK,GAAI,CAAC;IACpBW,KAAK,EAAErB,OAAO,CAACY,KAAK;IACpBI,SAAS,EAAE,KAAK;IAChBL,MAAM,EAAEV,WAAW,CAACW;EACtB;EACAb,KAAK,CAACc,IAAI,CAACM,IAAI;AACjB;AAEA,SAASG,OAAOA,CAACF,EAAE,EAAE;EACnB;EACA,MAAMG,KAAI,GAAIxB,KAAK,CAACyB,SAAS,CAAEL,IAAI,IAAKA,IAAI,CAACC,EAAC,KAAMA,EAAE;EACtD;EACArB,KAAK,CAAC0B,MAAM,CAACF,KAAK,EAAE,CAAC;EACrBhB,OAAO,CAACC,GAAG,CAACT,KAAK;AACnB;AAEAD,SAAS,CAAC,MAAM;EACdI,QAAQ,CAAC;AACX,CAAC;;AAED;AACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}