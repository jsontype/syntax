<!-- Google Font Api KEY-->
<meta name="google_font_api" content="{{ googleFontApi }}">

<!-- Favicon -->
<link rel="shortcut icon" href="{{path}}assets/images/favicon.ico" />

<!-- Library / Plugin Css Build -->
<link rel="stylesheet" href="{{path}}assets/css/core/libs.min.css" />

<!-- font-awesome css -->
<link rel="stylesheet" href="{{path}}assets/vendor/font-awesome/css/all.min.css" />

<!-- Iconly css -->
<link rel="stylesheet" href="{{path}}assets/vendor/iconly/css/style.css" />

<!-- Animate css -->
<link rel="stylesheet" href="{{path}}assets/vendor/animate.min.css" />

{{#compare isSwiperSlider '==' 'true'}}
<!-- SwiperSlider css -->
<link rel="stylesheet" href="{{path}}assets/vendor/swiperSlider/swiper.min.css">
{{/compare}}

{{#compare isFlatpickr '==' 'true'}}
<!-- Flatpickr css -->
<link rel="stylesheet" href="{{path}}assets/vendor/flatpickr/dist/flatpickr.min.css" />
{{/compare}}

{{#compare isSweetalert '==' 'true'}}
<!-- Sweetlaert2 css -->
<link rel="stylesheet" href="{{ path }}assets/vendor/sweetalert2/sweetalert2.min.css"/>
{{/compare}}

{{#compare isVideoJs '==' 'true'}}
<link href="{{path}}assets/vendor/video/video-js.css" rel="stylesheet" />
{{/compare}}

{{#compare isNoUISlider '==' 'true'}}
<!-- NoUI Slider css -->
<link rel="stylesheet" href="{{path}}assets/vendor/noUiSilder/style/nouislider.min.css">
{{/compare}}

<!-- Streamit Design System Css -->
<link rel="stylesheet" href="{{path}}assets/css/streamit.min.css?v={{version}}" />

<!-- Custom Css -->
<link rel="stylesheet" href="{{path}}assets/css/custom.min.css?v={{version}}" />

<!-- Rtl Css -->
<link rel="stylesheet" href="{{path}}assets/css/rtl.min.css?v={{version}}" />

<!-- Google Font -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,300;0,400;0,500;0,700;0,900;1,300&display=swap" rel="stylesheet">

